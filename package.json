{
  "name": "next-typescript-tailwind-starter",
  "description": "Starter Next.js project using TypeScript, Tailwind and some DevTools preconfigured.",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "analyze": "ANALYZE=true next build",
    "eslint": "eslint --cache --ext .js,.mjs,.cjs,.ts,.jsx,.tsx",
    "stylelint": "stylelint --cache",
    "prettier": "prettier",
    "lint": "concurrently -s -n lint: 'yarn:lint:*(!fix)'",
    "lint:ts": "tsc --noemit",
    "lint:eslint": "yarn eslint .",
    "lint:css": "yarn stylelint \"**/*.css\"",
    "lint:prettier": "yarn prettier --check .",
    "lint:fix": "concurrently -s -n lint:fix: 'yarn:lint:fix:*'",
    "lint:fix:eslint": "yarn eslint --fix .",
    "lint:fix:css": "yarn stylelint --fix \"**/*.css\"",
    "lint:fix:prettier": "yarn prettier --write ."
  },
  "dependencies": {
    "@next/bundle-analyzer": "13.2.4",
    "autoprefixer": "^10.4.14",
    "concurrently": "^7.6.0",
    "next": "13.2.4",
    "postcss": "^8.4.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.2.7"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.2.4",
    "@svgr/webpack": "^6.5.1",
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "^18.15.0",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.28",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-unicorn": "^46.0.0",
    "lefthook": "^1.5.0",
    "prettier": "^2.8.4",
    "stylelint": "^15.2.0",
    "stylelint-config-standard": "^30.0.1",
    "typescript": "^4.9.5"
  },
  "packageManager": "yarn@3.6.3"
}
