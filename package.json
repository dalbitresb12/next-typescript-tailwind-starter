{
  "name": "next-typescript-tailwind-starter",
  "description": "Starter Next.js project using TypeScript, Tailwind and some DevTools preconfigured.",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "analyze": "ANALYZE=true next build",
    "eslint": "eslint --cache --ext .js,.mjs,.cjs,.ts,.jsx,.tsx",
    "stylelint": "stylelint --cache",
    "prettier": "prettier",
    "lint": "concurrently -s -n lint: 'yarn:lint:*(!fix)'",
    "lint:ts": "tsc --noemit",
    "lint:eslint": "yarn eslint .",
    "lint:css": "yarn stylelint \"**/*.css\"",
    "lint:prettier": "yarn prettier --check .",
    "lint:fix": "concurrently -s -n lint:fix: 'yarn:lint:fix:*'",
    "lint:fix:eslint": "yarn eslint --fix .",
    "lint:fix:css": "yarn stylelint --fix \"**/*.css\"",
    "lint:fix:prettier": "yarn prettier --write ."
  },
  "dependencies": {
    "@next/bundle-analyzer": "13.5.2",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.1",
    "next": "13.5.2",
    "postcss": "^8.4.30",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.5.2",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.6",
    "@types/node": "^18",
    "@types/react": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "eslint-config-next": "13.5.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-unicorn": "^48.0.1",
    "lefthook": "^1.5.0",
    "prettier": "^3.0.3",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^4.9.5"
  },
  "packageManager": "yarn@3.6.3"
}
